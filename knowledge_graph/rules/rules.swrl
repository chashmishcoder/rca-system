// SWRL Rules for Predictive Maintenance\n\n// MANUFACTURING_RULES RULES\n// MFG_R001: Tool Wear Threshold Rule\nManufacturingEquipment(?e) ∧ hasToolWear(?e, ?wear) ∧ swrlb:greaterThan(?wear, 200) → hasFailure(?e, ToolWearFailure)\n\n// MFG_R002: Heat Dissipation Failure Rule\nManufacturingEquipment(?e) ∧ hasProcessTemperature(?e, ?pt) ∧ hasAirTemperature(?e, ?at) ∧ swrlb:subtract(?diff, ?pt, ?at) ∧ swrlb:greaterThan(?diff, 10) → hasFailure(?e, HeatDissipationFailure)\n\n// MFG_R003: Power Failure Rule\nManufacturingEquipment(?e) ∧ hasTorque(?e, ?torque) ∧ hasRotationalSpeed(?e, ?speed) ∧ swrlb:lessThan(?torque, 30) ∧ swrlb:greaterThan(?speed, 1600) → hasFailure(?e, PowerFailure)\n\n// MFG_R004: Quality-Based Maintenance Rule\nManufacturingEquipment(?e) ∧ hasQualityType(?e, ?qt) ∧ swrlb:equal(?qt, 'L') → requiresMaintenance(?e, PreventiveMaintenance)\n\n// TRANSPORTATION_RULES RULES\n// TRANS_R001: Compressor Failure Rule\nTransportationEquipment(?e) ∧ hasComponent(?e, ?comp) ∧ Compressor(?comp) ∧ hasStatus(?comp, 0) ∧ hasPressure(?e, ?pressure) ∧ swrlb:lessThan(?pressure, -0.05) → hasFailure(?comp, CompressorFailure)\n\n// TRANS_R002: Oil Temperature Anomaly Rule\nTransportationEquipment(?e) ∧ hasOilTemperature(?e, ?temp) ∧ swrlb:greaterThan(?temp, 70) → hasAnomaly(?e, TemperatureAnomaly)\n\n// TRANS_R003: Motor Current Spike Rule\nTransportationEquipment(?e) ∧ hasMotorCurrent(?e, ?current) ∧ swrlb:greaterThan(?current, 5.0) → hasAnomaly(?e, CurrentAnomaly)\n\n// CROSS_DOMAIN_RULES RULES\n// CROSS_R001: Cascade Failure Prevention Rule\nEquipment(?e) ∧ hasComponent(?e, ?c1) ∧ hasComponent(?e, ?c2) ∧ hasFailure(?c1, ?f) ∧ swrlb:notEqual(?c1, ?c2) → requiresInspection(?c2)\n\n// PHYSICS_RULES RULES\n// PHYS_R001: Thermal Expansion Rule\nEquipment(?e) ∧ hasTemperatureDifferential(?e, ?diff) ∧ swrlb:greaterThan(?diff, 15) → hasAnomaly(?e, ThermalStressAnomaly)\n\n// PHYS_R002: Energy Conservation Rule\nEquipment(?e) ∧ hasPowerConsumption(?e, ?power) ∧ hasWorkOutput(?e, ?work) ∧ hasEfficiency(?e, ?eff) ∧ swrlb:divide(?calc_eff, ?work, ?power) ∧ swrlb:lessThan(?calc_eff, 0.5) → hasAnomaly(?e, EfficiencyAnomaly)\n\n// STATISTICAL_RULES RULES\n// STAT_R001: Moving Average Deviation Rule\nSensor(?s) ∧ hasValue(?s, ?value) ∧ hasMovingAverage(?s, ?avg) ∧ hasStandardDeviation(?s, ?std) ∧ swrlb:subtract(?diff, ?value, ?avg) ∧ swrlb:abs(?abs_diff, ?diff) ∧ swrlb:multiply(?threshold, ?std, 2) ∧ swrlb:greaterThan(?abs_diff, ?threshold) → hasAnomaly(?s, StatisticalAnomaly)\n\n// EXPERT_RULES RULES\n// EXP_R001: Maintenance Schedule Rule\nEquipment(?e) ∧ hasAnomaly(?e, ?a1) ∧ hasAnomaly(?e, ?a2) ∧ swrlb:notEqual(?a1, ?a2) → requiresMaintenance(?e, ImmediateMaintenance)\n\n